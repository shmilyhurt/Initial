                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户信息接口"
                ],
                "summary": "用户信息",
                "operationId": "/user/create",
                "parameters": [
                    {
                        "type": "string",
