                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户信息接口"
                ],
                "summary": "登录",
                "operationId": "/user/Login",
                "parameters": [
                    {
                        "type": "string",
                        "name": "password",
